                      (       forth9.asm):00101         *            ASCII CHARACTERS USED               *
                      (       forth9.asm):00102         *                                                *
                      (       forth9.asm):00103         **************************************************
     0020             (       forth9.asm):00104         ABL     EQU   $0020          ; A blank
     00D0             (       forth9.asm):00105         ACR     EQU   $00D0          ; A carriage return
     002E             (       forth9.asm):00106         ADOT    EQU   $002E          ; A period
     0007             (       forth9.asm):00107         BELL    EQU   $0007          ; A bell (^G)
     005F             (       forth9.asm):00108         BSIN    EQU   $005F          ; An input delete
     0008             (       forth9.asm):00109         BSOUT   EQU   $0008          ; A output backspace
     0010             (       forth9.asm):00110         DLE     EQU   $0010          ; (^P)
     00A0             (       forth9.asm):00111         LF      EQU   $00A0          ; A line feed
     00C0             (       forth9.asm):00112         FF      EQU   $00C0          ; A form feed
                      (       forth9.asm):00113         
                      (       forth9.asm):00114         **************************************************
                      (       forth9.asm):00115         *                                                *
                      (       forth9.asm):00116         *               MEMORY ALLOCATION                *
                      (       forth9.asm):00117         *                                                *
                      (       forth9.asm):00118         **************************************************
     C000             (       forth9.asm):00119         RAMS    EQU   $C000          ; Start of RAM
     E000             (       forth9.asm):00120         EM      EQU   $E000          ; End of RAM + 1
     0000             (       forth9.asm):00121         NSCR    EQU   $0000          ; Number of 1024 byte screens
     0080             (       forth9.asm):00122         KBBUF   EQU   $0080          ; Data bytes per disk buffer
     0040             (       forth9.asm):00123         US      EQU   $0040          ; User variable space
     00A0             (       forth9.asm):00124         RTS     EQU   $00A0          ; Return stack and terminal buffer
     0084             (       forth9.asm):00125         CO      EQU   $0084          ; Data bytes per buffer plus 4
     0000             (       forth9.asm):00126         NBUF    EQU   $0000          ; Number of buffers
     E000             (       forth9.asm):00127         BUF1    EQU   $E000          ; First disk buffer
     DFC0             (       forth9.asm):00128         INITR0  EQU   BUF1-US        ; Base of return stack
     DF20             (       forth9.asm):00129         INITS0  EQU   INITR0-RTS     ; Base of parameter stack
                      (       forth9.asm):00130         
     9800             (       forth9.asm):00131         ACIAC   EQU   $9800          ; ACIA Control
     9801             (       forth9.asm):00132         ACIAD   EQU   $9801          ; ACIA Data
                      (       forth9.asm):00133         
                      (       forth9.asm):00134         **************************************************
                      (       forth9.asm):00135         *                                                *
                      (       forth9.asm):00136         *             START OF ROMABLE FORTH             *
                      (       forth9.asm):00137         *                                                *
                      (       forth9.asm):00138         **************************************************
                      (       forth9.asm):00139                 ORG   $E000
E000 8603             (       forth9.asm):00140         ORIG    LDA   #$03           ; Configure the ACIA
                      (       forth9.asm):00141         ;        STA   ACIAC
E002 8616             (       forth9.asm):00142                 LDA   #$16
                      (       forth9.asm):00143         ;        STA   ACIAC
E004 108EE013         (       forth9.asm):00144                 LDY   #ENTRY
E008 10CEDFA0         (       forth9.asm):00145                 LDS   #$DFA0         ; Initial stack pointers
E00C CEDF20           (       forth9.asm):00146                 LDU   #$DF20
E00F AEA1             (       forth9.asm):00147                 LDX   ,Y++           ; Set up is done, now do FORTH
E011 6E91             (       forth9.asm):00148                 JMP   [,X++]
E013 EF2B             (       forth9.asm):00149         ENTRY   FDB   COLD
                      (       forth9.asm):00150         
                      (       forth9.asm):00151         **************************************************
                      (       forth9.asm):00152         *                                                *
                      (       forth9.asm):00153         *                INITIAL USER TABLE              *
                      (       forth9.asm):00154         *                                                *
                      (       forth9.asm):00155         **************************************************
E015 C012             (       forth9.asm):00156         TOP     FDB   RTASK-7        ; Top word in FORTH vocabulary
E017 DF20             (       forth9.asm):00157         UTABLE  FDB   INITS0         ; Initial parameter stack base
E019 DFC0             (       forth9.asm):00158                 FDB   INITR0         ; Initial return stack base
E01B DF20             (       forth9.asm):00159                 FDB   INITS0         ; Initial TIB
E01D 0020             (       forth9.asm):00160                 FDB   32             ; Initial WIDTH
E01F 0000             (       forth9.asm):00161                 FDB   0              ; Initial WARNING
E021 C01D             (       forth9.asm):00162                 FDB   INITDP         ; Initial FENCE
E023 C01D             (       forth9.asm):00163                 FDB   INITDP         ; Initial DP
E025 C00E             (       forth9.asm):00164                 FDB   RFORTH+6       ; Initial VOC-LINK
E027 DFC0             (       forth9.asm):00165         UP      FDB   INITR0         ; Initial user area pointer
E029 DFC0             (       forth9.asm):00166         RPP     FDB   INITR0         ; Initial return stack pointer
                      (       forth9.asm):00167         
                      (       forth9.asm):00168         **************************************************
                      (       forth9.asm):00169         *                                                *
                      (       forth9.asm):00170         *           START OF THE FORTH DICTIONARY        *
                      (       forth9.asm):00171         *                                                *
                      (       forth9.asm):00172         **************************************************
E02B 83               (       forth9.asm):00173         DP0     FCB   $83
E02C 4C49             (       forth9.asm):00174                 FCC   'LI'
E02E D4               (       forth9.asm):00175                 FCB   $D4            ; 'T'+$80
E02F 0000             (       forth9.asm):00176                 FDB   $0000          ; LFA of zero indicates start of dictionary
E031 E033             (       forth9.asm):00177         LIT     FDB   *+2
E033 ECA1             (       forth9.asm):00178                 LDD   ,Y++           ; Get Literal
E035 3606             (       forth9.asm):00179                 PSHU  D              ; Push to user stack
E037 AEA1             (       forth9.asm):00180                 LDX   ,Y++           ; NEXT
E039 6E91             (       forth9.asm):00181                 JMP   [,X++]
                      (       forth9.asm):00182         
E03B 87               (       forth9.asm):00183                 FCB   $87
E03C 455845435554     (       forth9.asm):00184                 FCC   'EXECUT'
E042 C5               (       forth9.asm):00185                 FCB   $C5
E043 E02B             (       forth9.asm):00186                 FDB   LIT-6
E045 E047             (       forth9.asm):00187         EXEC    FDB   *+2
E047 3710             (       forth9.asm):00188                 PULU  X              ; Get CFA from parameter stack
E049 6E91             (       forth9.asm):00189                 JMP   [,X++]         ; Execute NEXT
                      (       forth9.asm):00190         
E04B 86               (       forth9.asm):00191                 FCB   $86
E04C 4252414E43       (       forth9.asm):00192                 FCC   'BRANC'
E051 C8               (       forth9.asm):00193                 FCB   $C8
E052 E03B             (       forth9.asm):00194                 FDB   EXEC-10
E054 E056             (       forth9.asm):00195         BRAN    FDB   *+2
E056 ECA4             (       forth9.asm):00196         BRAN1   LDD   ,Y             ; Get offset value
E058 31AB             (       forth9.asm):00197                 LEAY  D,Y            ; Add offset to instruction pointer
E05A AEA1             (       forth9.asm):00198                 LDX   ,Y++           ; NEXT
E05C 6E91             (       forth9.asm):00199                 JMP   [,X++]
                      (       forth9.asm):00200         
E05E 87               (       forth9.asm):00201                 FCB   $87
E05F 304252414E43     (       forth9.asm):00202                 FCC   '0BRANC'
E065 C8               (       forth9.asm):00203                 FCB   $C8
E066 E04B             (       forth9.asm):00204                 FDB   BRAN-9
E068 E06A             (       forth9.asm):00205         ZBRAN   FDB   *+2
E06A 3706             (       forth9.asm):00206                 PULU  D
E06C 10830000         (       forth9.asm):00207                 CMPD  #0             ; Is top of stack zero?
E070 27E4             (       forth9.asm):00208                 BEQ   BRAN1          ; Yes, must branch
E072 3122             (       forth9.asm):00209                 LEAY  2,Y            ; No, skip branch offset
E074 AEA1             (       forth9.asm):00210                 LDX   ,Y++           ; NEXT
E076 6E91             (       forth9.asm):00211                 JMP   [,X++]
                      (       forth9.asm):00212         
E078 86               (       forth9.asm):00213                 FCB   $86
E079 284C4F4F50       (       forth9.asm):00214                 FCC   '(LOOP'
E07E A9               (       forth9.asm):00215                 FCB   $A9
E07F E05E             (       forth9.asm):00216                 FDB   ZBRAN-10
E081 E083             (       forth9.asm):00217         XLOOP   FDB   *+2
E083 CC0001           (       forth9.asm):00218                 LDD   #$0001         ; Assume an increment of 1
E086 E360             (       forth9.asm):00219         XLOO1   ADDD  0,S            ; Add increment to loop index
E088 ED60             (       forth9.asm):00220                 STD   0,S            ; Put the new index on the stack
E08A 10A362           (       forth9.asm):00221                 CMPD  2,S            ; Compare index with limit
E08D 2DC7             (       forth9.asm):00222                 BLT   BRAN1          ; Branch if index < limit
E08F 3264             (       forth9.asm):00223                 LEAS  4,S            ; Drop the index and the limit
E091 3122             (       forth9.asm):00224                 LEAY  2,Y            ; Skip the branch offset
E093 AEA1             (       forth9.asm):00225                 LDX   ,Y++           ; NEXT
E095 6E91             (       forth9.asm):00226                 JMP   [,X++]
                      (       forth9.asm):00227         
E097 87               (       forth9.asm):00228                 FCB   $87
E098 282B4C4F4F50     (       forth9.asm):00229                 FCC   '(+LOOP'
E09E A9               (       forth9.asm):00230                 FCB   $A9
E09F E078             (       forth9.asm):00231                 FDB   XLOOP-9
E0A1 E0A3             (       forth9.asm):00232         XPLOO   FDB   *+2
E0A3 3706             (       forth9.asm):00233                 PULU  D              ; Get the increment value
E0A5 20DF             (       forth9.asm):00234                 BRA   XLOO1          ; Go do loop
                      (       forth9.asm):00235         
E0A7 84               (       forth9.asm):00236                 FCB   $84
E0A8 28444F           (       forth9.asm):00237                 FCC   '(DO'
E0AB A9               (       forth9.asm):00238                 FCB   $A9
E0AC E097             (       forth9.asm):00239                 FDB   XPLOO-10
E0AE E0B0             (       forth9.asm):00240         XDO     FDB   *+2
E0B0 3716             (       forth9.asm):00241                 PULU  D,X            ; Get initial index and limit values
E0B2 3416             (       forth9.asm):00242                 PSHS  X,D            ; Push to system stack
E0B4 AEA1             (       forth9.asm):00243                 LDX   ,Y++           ; NEXT
E0B6 6E91             (       forth9.asm):00244                 JMP   [,X++]
                      (       forth9.asm):00245         
E0B8 81               (       forth9.asm):00246                 FCB   $81
E0B9 C9               (       forth9.asm):00247                 FCB   $C9
E0BA E0A7             (       forth9.asm):00248                 FDB   XDO-7
E0BC E0BE             (       forth9.asm):00249         IDO     FDB   *+2
E0BE EC60             (       forth9.asm):00250                 LDD   0,S            ; Get the index
E0C0 3606             (       forth9.asm):00251                 PSHU  D              ; Push it to the parameter stack
E0C2 AEA1             (       forth9.asm):00252                 LDX   ,Y++           ; NEXT
E0C4 6E91             (       forth9.asm):00253                 JMP   [,X++]
                      (       forth9.asm):00254         
E0C6 85               (       forth9.asm):00255                 FCB   $85
E0C7 44494749         (       forth9.asm):00256                 FCC   'DIGI'
E0CB D4               (       forth9.asm):00257                 FCB   $D4
E0CC E0B8             (       forth9.asm):00258                 FDB   IDO-4
E0CE E0D0             (       forth9.asm):00259         DIGIT   FDB   *+2
E0D0 4F               (       forth9.asm):00260                 CLRA                 ; Clear high byte
E0D1 E643             (       forth9.asm):00261                 LDB   3,U            ; Ascii digit is low byte of 2nd on the stack
E0D3 C030             (       forth9.asm):00262                 SUBB  #$30           ; Convert to binary
E0D5 251B             (       forth9.asm):00263                 BLO   DIGI2          ; Not a number
E0D7 C10A             (       forth9.asm):00264                 CMPB  #$0A           ; Ascii 0-9?
E0D9 250A             (       forth9.asm):00265                 BLO   DIGI1          ; Yes, check against base
E0DB C111             (       forth9.asm):00266                 CMPB  #$11           ; Ascii A-Z?
